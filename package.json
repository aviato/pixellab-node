{
  "name": "pixellab-node",
  "version": "2.0.1",
  "description": "Node.js API for interacting with the [Pixel Lab Developer API](http://api.pixellab.ai/v1/docs)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm test",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "prepublishOnly": "npm run build",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "pixellab",
    "api",
    "node",
    "typescript",
    "javascript",
    "ai",
    "artificial intelligence",
    "pixel art"
  ],
  "author": "Ryan Price",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.13.8",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "dotenv": "^16.4.7"
  }
}
